{% extends '../../node_modules/@gov-cy/govcy-frontend-renderer/src/njk/layouts/govcyBase.njk' %}
{% from "../../node_modules/@gov-cy/govcy-frontend-renderer/src/njk/govcyElement.njk" import govcyElement %}

{% block main %}
{% call govcyElement("form",{id:"DSFPrototypeForm"} ) %}

<link rel="stylesheet" href="../../css/progress.css">
<div class="govcy-step-indicator__counter"><span class="govcy-visually-hidden">Step </span><span class="govcy-step-indicator__current-counter">2</span> of 2</div>
{{ 
    govcyElement(
        "textInput",
        {
            "label": {
                "en": "What is your phone number?",
                "el": "Σε ποιο τηλέφωνο μπορούμε να επικοινωνήσουμε μαζί σας;"
            },
            "id": "tel-no",
            "name": "tel-no",
            "hint": {
                "en": "For Cypriot phone number no country code is needed",
                "el": "Για κυπριακό τηλέφωνο δεν χρειάζεται κωδικός χώρας"
            },
            "type": "tel",
            "isPageHeading": true,
            "fixedWidth": "50",
            "lang": "en"
        }
    ) 
}}

{{govcyElement("button",{lang:"en",text:{en:"Continue",el:"Συνέχεια"},type:"submit"} ) }}
{% endcall %}
{% endblock %}