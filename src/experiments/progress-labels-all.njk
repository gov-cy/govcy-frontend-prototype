{% extends '../../node_modules/@gov-cy/govcy-frontend-renderer/src/njk/layouts/govcyBase.njk' %}
{% from "../../node_modules/@gov-cy/govcy-frontend-renderer/src/njk/govcyElement.njk" import govcyElement %}

{% block main %}
{% call govcyElement("form",{id:"DSFPrototypeForm"} ) %}

{# With labels #}
<h2>With labels</h2>
{{ 
     govcyElement("progressList",
    {
        "current":2
        ,"total":5
        ,"showSteps":true
        ,"steps": 
            [
                {"text": {"en":"Personal information","el":"Personal information"} },
                {"text": {"en":"Phone number","el":"Phone number"} },
                {"text": {"en":"Address","el":"Address"} },
                {"text": {"en":"Other details","el":"Other details"} },
                {"text": {"en":"Consent","el":"Other details"} }
            ]
    })
}}
{# No labels #}
<h2>No labels</h2>
{{ 
     govcyElement("progressList",
    {
        "current":2
        ,"total":5
        ,"showSteps":true
    })
}}
{# No labels, no segments #}
<h2>No labels, no segments</h2>
{{ 
     govcyElement("progressList",
    {
        "current":2
        ,"total":5
    })
}}

{% endcall %}
{% endblock %}